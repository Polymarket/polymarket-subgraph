{
  "name": "polymarket-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for Polymarket",
  "main": "index.js",
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/deploy/",
    "local:create": "graph create --node http://localhost:8020/ Polymarket/polymarket",
    "local:remove": "graph remove --node http://localhost:8020/ Polymarket/polymarket",
    "local:deploy": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 Polymarket/polymarket",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
    "deploy:matic": "yarn deploy TokenUnion/polymarket-matic",
    "deploy:matic-staging": "yarn deploy Polymarket/polymarket-matic-staging",
    "deploy:mumbai": "yarn deploy TokenUnion/polymarket-mumbai",
    "codegen": "ts-node ./templatify.ts && graph codegen --debug --output-dir src/types/",
    "codegen:matic": "NETWORK_NAME=matic yarn codegen",
    "codegen:mumbai": "NETWORK_NAME=mumbai yarn codegen",
    "publish-graph:matic": "yarn codegen:matic && yarn deploy:matic",
    "publish-graph:matic-staging": "yarn codegen:matic && yarn deploy:matic-staging",
    "publish-graph:mumbai": "yarn codegen:mumbai && yarn deploy:mumbai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TokenUnion/polymarket-subgraph.git"
  },
  "contributors": [
    {
      "name": "Tom French",
      "url": "https://github.com/tomafrench"
    },
    {
      "name": "Gnosis",
      "url": "https://gnosis.io"
    }
  ],
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/TokenUnion/polymarket-subgraph/issues"
  },
  "homepage": "https://github.com/TokenUnion/polymarket-subgraph#readme",
  "devDependencies": {
    "@gnosis.pm/conditional-tokens-contracts": "^1.0.1",
    "@gnosis.pm/conditional-tokens-market-makers": "^1.8.1",
    "@graphprotocol/graph-cli": "^0.18.0",
    "@types/fs-extra": "^9.0.2",
    "@types/js-yaml": "^3.12.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.18.1"
  }
}